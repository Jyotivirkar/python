#In dictionary to find hello word

d3={'key1':[{'nested_key':['this is a deep',['hello']]}]}
d3=['key1']
{'nested_key':['this is a deep',['hello']]}
d3[key1][0]['nested key']
['this is a deep,['hello']]
d3[key1][0]['nested key'][1]
['hello']
